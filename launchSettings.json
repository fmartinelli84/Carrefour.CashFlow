{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "carrefour.cashflow.api",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}/swagger",
      "serviceActions": {
        "carrefour.cashflow.web": "StartWithoutDebugging",
        "carrefour.cashflow.api": "StartDebugging",
        "carrefour.cashflow.jobs": "StartDebugging",
        "carrefour.cashflow.database": "StartWithoutDebugging",
        "carrefour.cashflow.logging": "StartWithoutDebugging"
      }
    }
  }
}